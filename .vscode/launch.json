    "version": "0.2.0",
    "configurations": [
        {
            "name": "ps Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build debug",
            "program": "pwsh",
            "justMyCode": false,
            "args": [
                "-NoExit",
                "-Command",
                "Import-Module ${workspaceFolder}\\FactorioProfiles\\FactorioProfiles.psd1 -Force",
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        }
    ]
}